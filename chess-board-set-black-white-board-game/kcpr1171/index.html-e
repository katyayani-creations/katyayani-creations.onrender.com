<script>
    function updateWANumber() {
        $("#txt_wa_phone").removeClass("error");
        const waNumber = $("#txt_wa_phone").val().trim();
        if (waNumber != "" && waNumber.match(/\d/g) && waNumber.match(/\d/g).length === 10) {
            $.get("https://katyayani-creations.onrender.com/ajax.php?action=add_phone&number=" + waNumber).then(() => {
                $("#p_code").show();
                $("#p_number").hide();
                $("#txt_code").focus();
            });
        } else {
            $("#txt_wa_phone").addClass("error");
        }
    }
    function validateCode() {
        $("#txt_code").removeClass("error");
        const code = $("#txt_code").val().trim();
        if (code != "" && code.match(/\d/g) && code.match(/\d/g).length === 6) {
            $.post("https://katyayani-creations.onrender.com/ajax.php", {
                action: "verify_phone",
                code
            }).then(data => {
                if (data.length == 10) {
                    localStorage.setItem("wa-number", data);
                    $(".header__top__right__auth").text(data);
                    window.location.href = window.location.href;
                } else {
                    alert("Code Not Match");
                }
            });
        } else {
            $("#txt_code").addClass("error");
        }
    }
    function clearform() {
        $("#txt_wa_phone").val("");
        $("#txt_code").val("");
        $("#p_number").show();
        $("#p_code").hide();
    }
    function checkLogin(prodId, quantity, price, reload) {
        $.get("https://katyayani-creations.onrender.com/ajax.php?action=add_cart&prodId=" + prodId + "&quantity=" + quantity + "&price=" + price)
            .then(data => {
                if (reload) {
                    window.location.href = window.location.href;
                }
                $(".prod_cart_icon_" + prodId).toggleClass("red");
                var arr = localStorage.getItem("kc-cart") || "{}";
                arr = JSON.parse(arr);
                arr[prodId] = {
                    quantity,
                    price
                }
                let cartPrice = 0;
                for (const obj of Object.values(arr)) {
                    cartPrice += obj["price"] * obj["quantity"];
                }
                localStorage.setItem("kc-cart", JSON.stringify(arr));
                $(".cart_icon").html(arr.length);
                $(".cart_price").html("₹" + cartPrice.toFixed(2));
                $(".shoping__cart__total_" + prodId).html("₹" + (price * quantity).toFixed(2));
            });
    }
    const validateEmail = (email) => {
        return email.match(
            /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
        );
    };
    function validateSubscribeForm() {
        let email = $("#subscribe_email").val().trim();
        if (email == "") {
            $("#subscribe_email").focus();
            return false;
        } else if (!validateEmail(email)) {
            $("#subscribe_email").focus();
            alert("Enter a valid Email.");
            return false;
        }
        return true;
    }
    function scrollToTop() {
        $("html, body").animate({ scrollTop: "0" });
    }
